<resources>
    <color name="_background">#ff090909</color>
    <color name="_accent">#ff63A088</color>
    <color name="_trans">#000000</color>
    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.

    -->
    <style name="AuraTheme" parent="Theme.AppCompat.NoActionBar">

        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowBackground">@color/_background</item>

    </style>

    <style name="AuraTheme.Transparent" parent="AuraTheme">

        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:backgroundDimEnabled">false</item>
    </style>


    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme"></style>

</resources>